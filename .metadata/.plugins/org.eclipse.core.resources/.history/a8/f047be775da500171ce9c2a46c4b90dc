package main;

import java.io.File;
import java.io.FilenameFilter;
import java.util.Arrays;

import javax.swing.JFileChooser;
import javax.swing.filechooser.FileFilter;

import main.listaEncadeada.ListaEncadeada;

public class Main {

	public static void main(String[] args) {
		
		File pastaInput = new File(ClassLoader.getSystemResource("input").getFile());
		File pastaOutput = new File(ClassLoader.getSystemResource("output").getFile());
		
		System.out.println(pastaInput.getPath() + " - " + pastaInput.isDirectory());
		System.out.println(pastaOutput.getPath() + " - " + pastaInput.exists());
		
		
	}

}
