package main.listaEncadeada;

public class ListaEncadeada<T> {

	private No<T> raiz;
	private No<T> fim;
	private int tam;
	
	public ListaEncadeada(){
		this.raiz = null;
		this.fim = null;
		this.tam = null;
	}
	
	
	
	public No<T> getRaiz() {
		return raiz;
	}
	public void setRaiz(No<T> raiz) {
		this.raiz = raiz;
	}
	public No<T> getFim() {
		return fim;
	}
	public void setFim(No<T> fim) {
		this.fim = fim;
	}
	public int getTam() {
		return tam;
	}
	public void setTam(int tam) {
		this.tam = tam;
	}
	
	
	
}
