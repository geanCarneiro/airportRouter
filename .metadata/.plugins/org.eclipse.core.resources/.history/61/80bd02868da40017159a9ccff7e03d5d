package main.listaEncadeada;

public class No<T> {
	
	private T info;
	private No<T> prox;
	
	public No(T elem){
		this.info = elem;
		this.prox = null;
	}
	
	public No(T elem, No<T> prox){
		this(elem);
		this.prox = prox;
	}
	
	public T getInfo() {
		return info;
	}
	public void setInfo(T info) {
		this.info = info;
	}
	public No<T> getProx() {
		return prox;
	}
	public void setProx(No<T> prox) {
		this.prox = prox;
	}
	
	
	
}
