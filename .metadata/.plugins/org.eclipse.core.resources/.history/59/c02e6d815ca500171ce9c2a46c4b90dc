package main;

import java.io.File;

public class FileFilter extends javax.swing.filechooser.FileFilter{

	@Override
	public boolean accept(File arg0) {
		// TODO Auto-generated method stub
		return arg0.getName().endsWith(".txt");
	}

	@Override
	public String getDescription() {
		// TODO Auto-generated method stub
		return null;
	}

}
